{
  "AllowedHosts": "localhost;360.gordon.edu",
  "AllowedOrigin": null,
  "AzureAd": {
    "Instance": null,
    "ClientId": null,
    "TenantId": null,
    "Audience": null
  },
  "BonAppetit": {
    "IssuerID": null,
    "ApplicationID": null,
    "Secret": null
  },
  "ConnectionStrings": {
    "CCT": null,
    "MyGordon": null,
    "StudentTimesheets": null
  },
  "DEFAULT_ACTIVITY_IMAGE_PATH": null,
  "DEFAULT_PROFILE_IMAGE_PATH": null,
  "PREFERRED_IMAGE_PATH": null,
  "DEFAULT_IMAGE_PATH": null,
  "DEFAULT_ID_SUBMISSION_PATH": null,
  "DATABASE_IMAGE_PATH": null,
  "Emails": {
    "Sender": {
      "Username": null,
      "Password": null
    },
    "AlumniProfileUpdateRequestApprover": null
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": {
      "ConsoleSink": "Console",
      "AsyncSinks": {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "./Logs/info-.json",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "rollingInterval": "Day",
                "retainedFileCountLimt": 14,
                "rollOnFileSizeLimit": true
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "./Logs/error-.json",
                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
                "rollingInterval": "Day",
                "restrictedToMinimumLevel": "Error"
              }
            }
          ]
        }
      }
    }
  },
  "SmtpHost": "smtp.gordon.edu"
}